package testes;

public class ExFamilia {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Familia rodrigo = new Familia();
		rodrigo.setNome("Rodrigo");
		rodrigo.setSobrenome("Curvello");
		
		Familia iram = new Familia();
		iram.setNome("Iram");
		iram.setSobrenome("Outra familia");
		
		System.out.println(rodrigo.getNome()+ ""+ rodrigo.getSobrenome());
		System.out.println(iram.getNome() + ""+ iram.getSobrenome());
		
	}

}
